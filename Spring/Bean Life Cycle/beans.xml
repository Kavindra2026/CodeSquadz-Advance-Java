<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
    default-init-method="init"
    default-destroy-method="destroy">

    <bean id="helloWorld" class="spring.Emp" init-method="init" destroy-method="destroy" scope="prototype">
        <property name="name" value="Bean Life Cycle"/>
    </bean>

    <bean id="helloWorld1" class="spring.Emp1">
        <property name="name" value="Bean Life Cycle1"/>
    </bean>
</beans>